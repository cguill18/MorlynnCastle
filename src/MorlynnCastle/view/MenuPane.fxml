<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.TilePane?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="vBoxRoot" alignment="CENTER" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MorlynnCastle.controller.MenuPaneController">
   <children>
      <TilePane alignment="CENTER" orientation="VERTICAL" vgap="20.0">
         <children>
            <Button maxHeight="Infinity" maxWidth="Infinity" mnemonicParsing="false" onAction="#start" text="Start" />
            <Button maxHeight="Infinity" maxWidth="Infinity" mnemonicParsing="false" onAction="#load" text="Continue" />
            <Button maxHeight="Infinity" maxWidth="Infinity" mnemonicParsing="false" onAction="#quit" text="Quit" />
         </children>
      </TilePane>
   </children>
</VBox>
